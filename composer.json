{
	"name": "myelophone/medoo",
	"type": "framework",
	"description": "Medoo with sqlite tuning and Distinct. Fork of the lightweight PHP database framework to accelerate the development.",
	"keywords": ["database", "database library", "lightweight", "PHP framework", "SQL", "MySQL", "MSSQL", "SQLite", "PostgreSQL", "MariaDB", "Oracle"],
	"homepage": "https://myelophone.com",
	"license": "MIT",
	"support": {
		"source": "https://github.com/Myelophone/Medoo"
	},
	"authors": [
		{"name": "Angel Lai", "email": "angel@medoo.in"},
		{"name": "Aleks Ivanov", "email": "mail@aleksivanov.me"}
	],
	"require": {
		"php": ">=7.3",
		"ext-pdo": "*"
	},
	"suggest": {
		"ext-pdo_mysql": "For MySQL or MariaDB database",
		"ext-pdo_sqlsrv": "For MSSQL database on both Window/Liunx platform",
		"ext-pdo_dblib": "For MSSQL or Sybase database on Linux/UNIX platform",
		"ext-pdo_oci": "For Oracle database",
		"ext-pdo_pqsql": "For PostgreSQL database",
		"ext-pdo_sqlite": "For SQLite database"
	},
	"autoload": {
		"psr-4": {
			"Medoo\\": "src/"
		}
	}
}